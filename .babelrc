{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3,
        "targets": {
          "electron": 12,
          "chrome": 95,
          "node": 10
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-syntax-throw-expressions",
    "@babel/plugin-proposal-export-default-from"
  ]
}